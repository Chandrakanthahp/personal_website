<%- include('partials/header') %>

<div class="outer_contact" style="padding-bottom: 1em;">
    <div class="outer_contact1">
        <div class="contact1 floating imagecontact">
            <center><h1 style="color: #5837D0;margin: 0;font-optical-sizing: auto;font-family: Inter;font-weight:400;display: inline;margin: 0;font-size:2.2em;" data-aos="fade-right" data-aos-duration="1500">Get in touch</h1></center>
            <img data-aos="fade-right" data-aos-duration="2000" id="imagecontact" class="imagecontact"  src="imagecontact.png" alt="">

        </div>
        <div class="contact2" style="border-left:solid #5837D0 2px;background-color: #795eda44;border-radius: 0.7em;"data-aos="zoom-in"data-aos-duration="2000">
            <form class="formdivision"  action="https://formsubmit.co/chandrakantha628@gmail.com" method="post" style="padding: 1em 2em 1em 1em;">
    <label for="fname" style="color:#5837D0;">First Name *</label><br>
        <input type="text" id="fname" class="fname" style="height: 26px;width: calc(100% - 30px); max-width: 90%;border-radius: 0.5em;border-color: #5837D0;" required placeholder="First name" name="fname" autocomplete="on"><br>
        <label for="lname" style="color: #5837D0;">Last Name *</label><br>
        <input type="text" id="lname" class="lname" style="height: 26px;width: calc(100% - 30px); max-width: 90%;border-radius: 0.5em;border-color: #5837D0;" required placeholder="Last name" name="lname" autocomplete="on"><br>
    
    
        
    
    
        <label style="color:#5837D0 ;" for="email">Email *</label><br>
        <input id="email" type="email" style="height: 26px;width: calc(100% - 30px); max-width: 90%;border-radius: 0.5em;border-color: #5837D0;" required placeholder="Enter your email here" name="email" autocomplete="on"><br>
    
    
        <label style="color: #5837D0;" for="message">Message</label><br>
        <textarea name="message" id="message" cols="70" rows="10" style="color: black;border-radius: 0.5em;border-color: #5837D0;" name="message" placeholder="Enter your message here"></textarea><br>
    
    
        
        <center><button class="submitbutton" id="submitbutton" >Submit ></button></center>
    
    </form>

        </div>
    </div>
</div>
<%- include('partials/footer') %>